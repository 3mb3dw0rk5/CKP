{
	"name": "chrome-keepass",
	"version": "0.9",
	"manifest_version": 2,
	"minimum_chrome_version": "37",
	"description": "Read-only KeePass integration via Google Docs",
	"icons": {
		"16": "icons/lock2_16.png",
		"48": "icons/lock2_48.png"
	},

	"oauth2": {
	    "client_id": "38107774587-ormlfo8t9bdd3f3lf49auam7qj9467u6.apps.googleusercontent.com",
	    "scopes": [
	      "https://www.googleapis.com/auth/drive.readonly"
	    ]
	  },

	"page_action": {
          "default_icon": {
            "19": "icons/lock2_19.png",
            "38": "icons/lock2_38.png"
          },
          "default_title": "Password field detected.  Click here to access your password file.",
		      "default_popup": "popups/popup.html"
        },

	"options_page": "options/options.html",
	"background": {
		"scripts": [
			"background/background.js",
			"lib/jquery-2.1.1.min.js"
			],
		"persistent": false
		},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"permissions": [
	  "activeTab",
	  "storage",
	  "clipboardWrite",
	  "identity",
	  "declarativeContent",

    "https://docs.google.com/feeds/",
    "https://docs.googleusercontent.com/",
    "https://spreadsheets.google.com/feeds/",
    "https://ssl.gstatic.com/",
    "https://www.googleapis.com/"

	]
}
