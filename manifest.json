{
	"name": "ChromeKeePass",
	"version": "2015.1.18.1",
	"manifest_version": 2,
	"minimum_chrome_version": "37",

	"description": "KeePass password database for Chrome",
	"icons": {
		"16": "icons/lock2_16.png",
		"48": "icons/lock2_48.png"
	},
	"incognito": "split",
  "key": "",
  "oauth2": {
	    "client_id": "38107774587-ormlfo8t9bdd3f3lf49auam7qj9467u6.apps.googleusercontent.com",
	    "scopes": [
	      "https://www.googleapis.com/auth/drive.readonly"
	    ]
	  },

	"page_action": {
          "default_icon": {
            "19": "icons/lock2_19.png",
            "38": "icons/lock2_38.png"
          },
          "default_title": "Password field detected.  Click here to access your KeePass passwords.",
		      "default_popup": "popups/popup.html"
        },

	"options_page": "options/options.html",
	"background": {
		"scripts": [
			"background/background.js"
			],
		"persistent": false
		},
	"permissions": [
	  "activeTab",
	  "storage",
	  "clipboardWrite",
	  "identity",
	  "declarativeContent",
	  "alarms"
	],
	"optional_permissions": [
    "https://*/"
	]

}
